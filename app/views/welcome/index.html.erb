<div id="user-creation">
<% if current_user %>
<%= link_to "Log Out", logout_path %>
    <h4>Existing Users</h4>
    <% @users.each do |user| %>
    <p><%= user.email%></p>
    <% end %>
<% else %>
    <%= button_to "Create A New User", new_register_path, method: :get %>
    <%= link_to "Log In", login_path %>
  <% end %>
</div>




